<?xml version="1.0" encoding="UTF-8"?>

<manifest>
    <remote name="vendor"
            fetch="https://github.com"
            revision="lineage-17.1" />
	
    <remote name="device"
            fetch="https://github.com"
            revision="rb"/>

    <remote name="kernel"
            fetch="https://github.com"
            revision="lineage-17.1"/>

    <remote name="apps_apk"
            fetch="https://github.com"
            revision="lineage-17.1"/>

    <!--Device Trees-->
    <project path="device/oppo/A37" name="OPPO-A37/rb_device_oppo_A37" remote="device" />

    <!--Kernel-->
    <project path="kernel/oppo/msm8939" name="OPPO-A37/kernel_oppo_A37-old" remote="kernel" />

    <!-- Vendor folders -->
    <project path="vendor/oppo" name="OPPO-A37/rb-vendor_oppo" remote="vendor" />

    <!-- Apps apk folders -->
    <project path="packages/apps/package_name" name="NTTKDeveloper/android_packages_apps_package_name" remote="apps_apk" />

    <project path="packages/apps/power_button" name="NTTKDeveloper/android_packages_apps_power_button" remote="apps_apk" />

    <project path="packages/apps/system_remover" name="NTTKDeveloper/android_packages_apps_system_remover" remote="apps_apk" />

    <project path="packages/apps/termux" name="NTTKDeveloper/android_packages_apps_termux" remote="apps_apk" />
</manifest>
